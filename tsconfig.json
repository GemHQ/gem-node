{
  "compilerOptions": {
    "declaration": true,
    "lib": ["es5"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitUseStrict": false,
    "noLib": false,
    "outDir": "dist",
    "removeComments": true,
    "rootDir": "src",
    "suppressImplicitAnyIndexErrors": true,
    "target": "es5"
  }
}
